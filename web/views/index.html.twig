{% extends "layout.html" %}
{% block content %}
<body ng-app="custom">
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#"><img src="images/dust-logo.png" border=0 alt="Dust" class="img-responsive" width=80 style="position:relative;top:-8px;"></a>
            </div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="#">Custom</a></li>
                <li><a href="#">Login</a></li>
                <li><a href="#">Sign up</a></li>
            </ul>
        </div>
    </nav>
    <div class="container" ng-controller="mvp">
        <h1 class="text-center">Dust Custom</h1>
        <div class="well well-lg">
            <form>
                <div class="form-group">
                    <label for="email">Email</label>
                    <input type="email" name="email" id="email" placeholder="Enter a valid email ID" class="form-control">
                </div>
            </form>
        </div>
    </div>
    <div class="container-fluid">
        <div id="myModal" class="modal fade" role="dialog"></div>
    </div>
</body>
{% endblock %}